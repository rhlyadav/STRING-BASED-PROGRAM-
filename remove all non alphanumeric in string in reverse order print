 
import java.io.*;
 import java.util.*;
 
class Rhl
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		String s=sc.next();
		s=s.replaceAll("[^a-zA-Z0-9]","");
		String rev="";
		for(int i=s.length()-1;i>=0;i--)
	{
		rev=rev+s.charAt(i);
	}
	System.out.println(rev);
	}
}
